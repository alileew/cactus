<footer id="footer">
  <div class="footer-left">
    <%= __('footer.copyright') %> &copy; <% var endYear = (theme.copyright &&
    theme.copyright.end_year) ? theme.copyright.end_year : new
    Date().getFullYear() %> <% var startYear = (theme.copyright &&
    theme.copyright.start_year) ? theme.copyright.start_year : new
    Date().getFullYear() %> <%= startYear >= endYear ? endYear : startYear + "-"
    + endYear %>
    <%= config.author || config.title %>
  </div>
  <div class="footer-right">
    <nav>
      <ul>
        <% for (var i in theme.nav) { %><!--
       -->
        <li>
          <a href="<%- url_for(theme.nav[i]) %>"
            ><%= __('nav.'+i).replace("nav.", "") %></a
          >
        </li>
        <!--
     --><% } %>
      </ul>
    </nav>
  </div>
  <div style="width: 100%; margin-top: 10px; text-align: center">
    <span id="busuanzi_container_site_pv" style='display:none'>
      你的努力, 值得被更多人看见, 正如现在努力的你 
      <i class="fa fa-eye" aria-hidden="true"></i>
      <span id="busuanzi_value_site_pv"></span>
    </span> 
    <!-- <span id="busuanzi_container_site_uv" style='display:none'>
      <i class="fa fa-user" aria-hidden="true"></i>
      <span id="busuanzi_value_site_uv"></span>
    </span> -->
  </div>
</footer>
